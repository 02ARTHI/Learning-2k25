import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
      Scanner sc= new Scanner(System.in);
        int n= sc.nextInt();
         int a=0;
         int r;
          while(n>0)
          {
            r=n%10;
            a=a*10+r;
            n=n/10;
          }
          System.out.println(a);
         
  }
}


