import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
      Scanner sc= new Scanner(System.in);
        int n= sc.nextInt();
        int p=0,c=1,result=0;
        for(int i=2;i<=n;i++)
        {
          result=p+c;
          p=c;
          c=result;
        }
          System.out.println(result);
         
  }
}
